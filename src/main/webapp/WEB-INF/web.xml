<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  <servlet-name>Journey</servlet-name>
  <servlet-class>MAIN.JourneyDetails</servlet-class>
  <init-param>
  	<param-name>PuneToMumbai</param-name>
  	<param-value>500</param-value>
  </init-param>
  
  <init-param>
  	<param-name>PuneToBanglore</param-name>
  	<param-value>3500</param-value>
  </init-param>
  <init-param>
  	<param-name>PuneToChennai</param-name>
  	<param-value>2500</param-value>
  </init-param>
  
  <init-param>
  	<param-name>PuneToNagpur</param-name>
  	<param-value>1500</param-value>
  </init-param>
  </servlet>
  <servlet-mapping>
  <servlet-name>Journey</servlet-name>
  <url-pattern>/journey</url-pattern>
  </servlet-mapping>
</web-app>
